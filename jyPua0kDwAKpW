        <!-- BEGIN JAVASCRIPT -->
        <script type="text/javascript">

            var GiphyEmbed = {
                loadImg: false,
                image_url: '',
                image_still_url: '',
                isFullScreen: false,

                initialize: function(){
                    var self = this;
                    this.loadOriginal();
                    this.html5Player();

                    // bind hover
                    jQuery('.embed').on('mouseover', function(event) { jQuery('body').addClass('hover'); });
                    jQuery('.embed').on('mouseout', function(event) { jQuery('body').removeClass('hover'); });

                    // bind fullscreen button
                    jQuery('a.fullscreen').on('click',function(e){
                        self.toggleFullScreen();
                        return false;
                    });

                    // bind fullscreen change events
                    jQuery(document).on('mozfullscreenchange', function(event){
                        self.isFullScreen = (document.mozFullScreen) ? true : false;
                        self.fullScreenChange(event);
                    });
                    jQuery(document).on('webkitfullscreenchange', function(event){
                        self.isFullScreen = (document.webkitIsFullScreen) ? true : false;
                        self.fullScreenChange(event);
                    });
                },

                sizeToFrame: function(){
                    var frame = $(window),
                        img = jQuery('img.gif, #gif-mp4'),
                        gifHeight = img.data("height"),
                        gifWidth = img.data("width"),
                        frameWidth =  frame.width(),
                        frameHeight = frame.height(),
                        gifRatio = gifWidth / gifHeight,
                        frameRatio = frameWidth / frameHeight,
                        heightOffset,
                        widthOffset,
                        scaleWidth,
                        scaleHeight;
                    if (gifRatio >= frameRatio) {
                        scaleWidth = (frameHeight / gifHeight) * gifWidth;
                        scaleHeight = frameHeight;
                        widthOffset = -(scaleWidth - frameWidth) / 2;
                        heightOffset = 0;
                    } else {
                        scaleWidth = frameWidth;
                        scaleHeight = (frameWidth / gifWidth) * gifHeight;
                        widthOffset = 0;
                        heightOffset = -(scaleHeight - frameHeight) / 2;
                    }
                    img.css({
                        "margin-top": heightOffset,
                        "margin-left": widthOffset
                    });
                    img.attr({
                        "height": scaleHeight,
                        "width": scaleWidth,
                    })
                },

                getParameterByName: function(name) {
                    name = name.replace(/[\[]/, "\\[").replace(/[\]]/, "\\]");
                    var regex = new RegExp("[\\?&]" + name + "=([^&#]*)"),
                        results = regex.exec(location.search);
                    return results == null ? "" : decodeURIComponent(results[1].replace(/\+/g, " "));
                },

                loadOriginal: function(){
                    var self = this;
                    var img = jQuery('img.gif');
                    this.image_url = img.attr('src');
                    this.image_still_url = img.data('still');

                    // begin loading
                    jQuery('body').addClass('loading');
                    img.attr('src',this.image_still_url);

                    // Load animated version outside of DOM and trigger complete method when finished
                    this.loadImg = jQuery("<img />").load(function(){
                        self.loadComplete();
                    }).attr('src', this.image_url);
                },

                loadComplete: function(){
                    var img = jQuery('img.gif'),
                        self = this;

                    // hide loader image
                    jQuery('body').removeClass('loading');
                    jQuery(".loader").hide();
                    // remove out-of-dom image object
                    self.loadImg.remove();
                    self.loadImg = false;

                    if (self.getParameterByName("playOnHover")) {
                        var gif = jQuery("#gif");
                        gif.on("tap", function(e) {
                            if (gif.attr("src") == gifattr("data-animated")) {
                                gif.attr("src", gif.attr("data-still"));
                            } else {
                                gif.attr("src", gifattr("data-animated"));
                            }
                        });
                        gif.hover(function(e) {
                            gif.attr("src", gif.attr("data-animated"));
                        }, function() {
                            gif.attr("src", gif.attr("data-still"));
                        });
                    } else {
                        img.attr('src',this.image_url);
                    }
                },

                canPlayMP4: function() {
                    return !!document.createElement('video').canPlayType('video/mp4; codecs=avc1.42E01E,mp4a.40.2');
                },

                html5Player: function() {
                    var self = this;
                    if(self.getParameterByName("html5") === 'true') {
                        var canPlayMP4 = self.canPlayMP4(),
                            gif = jQuery('img#gif'),
                            gifWrap = jQuery('div#embed');
                        if(canPlayMP4 === true) {
                            gifWrap.empty();
                            // inject the video html so work in IE9
                            gifWrap.html(
                                '<video id="gif-mp4" data-width="'+gif.data('width') +'" data-height="'+ gif.data('height') + '"' +
                                'poster="' + gif.data('still') + '"style="margin:0;padding:0" width="100%" height="100%" autoplay loop>' +
                                '<source src="' + gif.data('mp4_url') + '" type="video/mp4"></source>' +
                                '</video>'
                            );
                        }
                        self.sizeToFrame();
                        jQuery(window).on('resize',function(event){
                            self.sizeToFrame();
                        });

                    }
                },

                toggleFullScreen: function() {
                    if (!document.fullscreenElement && !document.mozFullScreenElement && !document.webkitFullscreenElement) {
                        if (document.documentElement.requestFullscreen) {
                            document.documentElement.requestFullscreen();
                        } else if (document.documentElement.mozRequestFullScreen) {
                            document.documentElement.mozRequestFullScreen();
                        } else if (document.documentElement.webkitRequestFullscreen) {
                          document.documentElement.webkitRequestFullscreen(Element.ALLOW_KEYBOARD_INPUT);
                        }
                    } else {
                        if (document.cancelFullScreen) {
                            document.cancelFullScreen();
                        } else if (document.mozCancelFullScreen) {
                            document.mozCancelFullScreen();
                        } else if (document.webkitCancelFullScreen) {
                            document.webkitCancelFullScreen();
                        }
                    }
                },

                fullScreenChange: function(event){
                    if(this.isFullScreen){
                        jQuery('body').addClass('fullscreen');
                    } else {
                        jQuery('body').removeClass('fullscreen');
                    }
                }
            };

            // LET'S GET THIS PARTY STARTED, RIGHT?
            if(typeof jQuery != "undefined") {
                jQuery(document).ready(function() {
                    GiphyEmbed.initialize();
                });
            }
        </script>
